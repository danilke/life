<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8" />
  <title><%= @page_title %></title>
  <meta name="description" content="<%= @page_description %>" />
  <meta name="keywords" content="<%= @page_keywords %>" />
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
	<div class="md">
		<div class="wrapper">
			<%= render :partial => 'structure/header' %>
			<div class="girls-list">
				<%= render :partial => 'structure/filter' %>
				<div class="items_wrapper">
					<div class="items">
						<%= yield %>
					</div>
				<span class="tide"></span>
			</div>
			<nav>
					<div class="compare left">
						<a href="/">Сравнить выбранных</a>
						<a href="/"><img src="/images/tr-arrow_icon.png" alt=""></a>
						<a href="/"><img src="/images/tr-arrow_icon.png" alt=""></a>
						<a href="/"><img src="/images/tr-arrow_icon.png" alt=""></a>
					</div>
					<div class="page-arrows right">
						<a href=""><img src="/images/db-arrow-right.png" alt=""></a>
						<a href=""><img src="/images/arrow-right.png" alt=""></a>
					</div>
					<div class="menu right">
						<ul>
							<li><a href="">1</a></li>
							<li class="active">2</li>
							<li><a href="">3</a></li>
							<li><a href="">4</a></li>
							<li><a href="">5</a></li>
							<li>...</li>
							<li><a href="">12</a></li>
						</ul>
					</div>
					<div class="page-arrows right">
						<a href=""><img src="/images/db-arrow-left.png" alt=""></a>
						<a href=""><img src="/images/arrow-left.png" alt=""></a>
					</div>
					<span class="tide"></span>
				</nav>
				<%= render :partial => 'models/top_models' %>
			</div>
			<%= render :partial => 'structure/footer' %>
		</div>
	</div>
</body>
</html>